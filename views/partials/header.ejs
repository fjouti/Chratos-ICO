<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Home</title>
    <link href="libs/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" href="styles/hover.css">
    <link rel="stylesheet" href="libs/OwlCarousel2-2.2.0/dist/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="fonts/fonts.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js">
    <link rel="stylesheet" href="styles/finance.css">
    <!--<link rel="shortcut icon" href="favicon.ico" type="image/x-icon">-->
</head>
<body>

<header class="site-header">
    <div class="cont">
        <div class="left-block">
            <div class="logo-block">
                <a href="#"><img src="images/site-logo.png" alt=""></a>
            </div>
        </div>
        <div class="right-block">
            <div class="burger-block">
                <button type="button" class="btn btn-default header-btn" data-toggle="modal" data-target="#myModalRegister">REGISTER</button>
                <button type="button" class="btn btn-default header-btn">BUY</button>
                <button type="button" class="btn btn-default header-btn slack-btn">SLACK</button>
            </div>
        </div>
    </div>

    <div class="site-menu">
        <ul>
            <li class="active"><a href="/">home</a></li>
            <% if(typeof(user) !== 'undefined') { %>
                <li><a href="/profile">profile</a></li>
                <li><a href="/signout">logout</a></li>
            <% } %>

            <div class="close-burger">
                <span></span>
                <span></span>
            </div>
        </ul>
    </div>
    <% if(typeof(errorMessage) !== 'undefined') {%>
        <div class="error-not"><%= errorMessage %></div>
    <% } %>
</header>